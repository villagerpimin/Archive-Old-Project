.frame {
  width: 92%;
  margin: auto;
}
.frame .out {
  position: relative;
  top: 30rpx;
  width: 100%;
  height: 150rpx;
  margin: auto;
  border-bottom: 1px solid rgba(128, 128, 128, 0.2);
}
.frame .out .outin {
  width: 90%;
  height: 100%;
  margin: auto;
  display: flex;
  justify-content: space-between;
  line-height: 130rpx;
}
.frame .out .outin .name {
  font-size: 18rpx;
}
.frame .out image {
  width: 95rpx;
  height: 95rpx;
  border-radius: 50%;
  border: 1px solid rgba(128, 128, 128, 0.1);
  margin-right: 60rpx;
}
.save {
  position: fixed;
  bottom: 25rpx;
  width: 100%;
  height: 70rpx;
  text-align: center;
}
.save .in {
  width: 50%;
  height: 100%;
  background-color: #4cd964;
  border-radius: 20rpx;
  line-height: 60rpx;
  color: #FFFFFF;
  margin: auto;
}

